{
  "name": "@mcp-hack/typescript",
  "version": "0.1.0",
  "description": "Open, vendor-agnostic observability SDK for Model Context Protocol (MCP) applications - TypeScript/JavaScript",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "observability",
    "llm",
    "ai",
    "monitoring",
    "tracing",
    "typescript",
    "javascript"
  ],
  "author": "MCP Observability Team <team@mcp-hack.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anish808/mcp-hack.git",
    "directory": "sdk/typescript"
  },
  "homepage": "https://github.com/anish808/mcp-hack#readme",
  "bugs": {
    "url": "https://github.com/anish808/mcp-hack/issues"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
} 